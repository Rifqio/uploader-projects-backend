{
    "compilerOptions": {
        "strict": true,
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "paths": {
            "@controller/*": ["./src/controller/*"],
            "@routes/*": ["./src/routes/*"],
            "@service": ["./src/service/*"],
            "@utils/*": ["./src/utils/*"],
            "@proto/*": ["./src/proto/*"],
            "@db/*": ["./src/database/*"],
            "@middleware/*": ["./src/middleware/*"],
            "@types/*": ["./src/types/*"],
            "@interface/*": ["./src/interface/*"],
        },
        "moduleResolution": "node"
    }
}
